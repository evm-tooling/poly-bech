        },
        {
          "include": "#comment"
        }
      ]
    },
    "setup-import-python-block": {
      "name": "meta.block.import.python.bench",
      "contentName": "source.python.embedded.bench",
      "begin": "^(\\s*)(import)\\s*\\{",
      "end": "^\\1\\}",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.setup.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "source.python"
        }
