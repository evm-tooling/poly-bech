      "begin": "^(\\s*)(setup)\\s+(python|py)\\s*\\{\\s*$",
      "end": "^\\1\\}\\s*$",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.bench"
        },
        "3": {
