        },
        {
          "include": "#setup-use-rust-block"
        },
        {
          "include": "#setup-init-rust-block"
        },
        {
          "include": "#setup-helpers-rust-block"
        },
        {
          "include": "#comment"
        }
      ]
    },
    "setup-import-rust-block": {
      "name": "meta.block.import.rust.bench",
      "begin": "^(\\s*)(import)\\s*\\{",
      "end": "^\\1\\}",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.setup.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "source.rust"
        }
      ]
    },
    "setup-declare-rust-block": {
      "name": "meta.block.declare.rust.bench",
      "begin": "^(\\s*)(declare)\\s*\\{",
      "end": "^\\1\\}",
      "beginCaptures": {
