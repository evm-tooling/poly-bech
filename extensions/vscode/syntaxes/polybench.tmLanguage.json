        }
      ]
    },
    "setup-rust-block": {
      "name": "meta.block.setup.rust.bench",
      "begin": "^(\\s*)(setup)\\s+(rust)\\s*\\{\\s*$",
      "end": "^\\1\\}\\s*$",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.bench"
        },
        "3": {
          "name": "entity.name.language.bench"
        }
      },
      "endCaptures": {
        "0": {
