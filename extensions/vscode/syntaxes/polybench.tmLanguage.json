{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "poly-bench",
  "scopeName": "source.bench",
  "patterns": [
    {
      "include": "#comment"
    },
    {
      "include": "#use-statement"
    },
    {
      "include": "#global-setup-block"
    },
    {
      "include": "#suite-block"
    }
  ],
  "repository": {
    "comment": {
      "name": "comment.line.bench",
      "match": "#.*$"
    },
    "use-statement": {
      "name": "meta.import.bench",
      "match": "\\b(use)\\s+(std)(::)([a-zA-Z_][a-zA-Z0-9_]*)",
      "captures": {
        "1": {
          "name": "keyword.control.import.bench"
        },
        "2": {
          "name": "entity.name.namespace.bench"
        },
        "3": {
          "name": "punctuation.accessor.bench"
        },
        "4": {
          "name": "entity.name.module.bench"
        }
      }
    },
    "global-setup-block": {
      "name": "meta.block.globalsetup.bench",
      "begin": "\\b(globalSetup)\\s*\\{",
      "end": "\\}",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "#comment"
        },
        {
          "include": "#stdlib-function-call"
        }
      ]
    },
    "stdlib-function-call": {
      "patterns": [
        {
          "name": "meta.function-call.stdlib.bench",
          "match": "\\b(anvil|constants|charting|math)\\s*\\.\\s*([a-zA-Z_][a-zA-Z0-9_]*)",
          "captures": {
            "1": {
              "name": "entity.name.module.bench"
            },
            "2": {
              "name": "entity.name.function.bench"
            }
          }
        },
        {
          "name": "meta.function-call.bench",
          "match": "\\b(spawnAnvil)\\s*\\(",
          "captures": {
            "1": {
              "name": "entity.name.function.bench"
            }
          }
        },
        {
          "name": "meta.function-call.charting.bench",
          "match": "\\b(drawSpeedupChart|drawTable|drawLineChart|drawBarChart)\\s*\\(",
          "captures": {
            "1": {
              "name": "entity.name.function.bench"
            }
          }
        }
      ]
    },
    "suite-global-setup-block": {
      "name": "meta.block.globalsetup.suite.bench",
      "begin": "\\b(globalSetup)\\s*\\{",
      "end": "\\}",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "#comment"
        },
        {
          "include": "#stdlib-function-call"
        },
        {
          "name": "string.quoted.double.bench",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.bench",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "suite-block": {
      "begin": "\\b(?:(declare)\\s+)?(suite)\\s+([a-zA-Z_][a-zA-Z0-9_]*)(?:\\s+(performance|memory)\\s+(timeBased|iterationBased)\\s+(sameDataset)\\s*:\\s*(true|false))?\\s*\\{",
      "end": "^\\}\\s*$",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.bench"
        },
        "2": {
          "name": "keyword.control.bench"
        },
        "3": {
          "name": "entity.name.section.bench"
        },
        "4": {
          "name": "constant.language.bench"
        },
        "5": {
          "name": "constant.language.bench"
        },
        "6": {
          "name": "keyword.other.bench"
        },
        "7": {
          "name": "constant.language.boolean.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "#comment"
        },
        {
          "include": "#suite-attributes"
        },
        {
          "include": "#suite-global-setup-block"
        },
        {
          "include": "#setup-go-block"
        },
        {
          "include": "#setup-ts-block"
        },
        {
          "include": "#setup-rust-block"
        },
        {
          "include": "#setup-python-block"
        },
        {
          "include": "#fixture-block"
        },
        {
          "include": "#bench-block"
        },
        {
          "include": "#suite-after-block"
        }
      ]
    },
    "suite-after-block": {
      "name": "meta.block.after.bench",
      "begin": "\\b(after)\\s*\\{",
      "end": "\\}",
      "beginCaptures": {
        "1": {
          "name": "keyword.control.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "#comment"
        },
        {
          "include": "#stdlib-function-call"
        },
        {
          "include": "#charting-params"
        }
      ]
    },
    "charting-params": {
      "patterns": [
        {
          "match": "\\b(title|description|sortBy|sortOrder|output|filterWinner|baselineBenchmark|baseline|theme|regressionModel|yScale)\\s*:\\s*",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            }
          }
        },
        {
          "match": "\\b(width|rowCount|height|limit)\\s*:\\s*(\\d+)",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.numeric.bench"
            }
          }
        },
        {
          "match": "\\b(minSpeedup)\\s*:\\s*([\\d.]+)",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.numeric.bench"
            }
          }
        },
        {
          "match": "\\b(includeBenchmarks|excludeBenchmarks)\\s*:",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            }
          }
        },
        {
          "match": "\\b(showStdDev|showErrorBars|showRegression)\\s*:\\s*(true|false)",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.boolean.bench"
            }
          }
        },
        {
          "name": "string.quoted.double.bench",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.bench",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "suite-attributes": {
      "patterns": [
        {
          "match": "\\b(description)\\s*:\\s*",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            }
          }
        },
        {
          "match": "\\b(iterations|warmup|count|fairnessSeed|asyncWarmupCap|asyncSampleCap)\\s*:\\s*(\\d+)",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.numeric.bench"
            }
          }
        },
        {
          "match": "\\b(timeout|targetTime)\\s*:\\s*(\\d+)(ms|s|m)?",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.numeric.bench"
            },
            "3": {
              "name": "keyword.other.unit.bench"
            }
          }
        },
        {
          "match": "\\b(cvThreshold)\\s*:\\s*(\\d+\\.?\\d*)",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.numeric.bench"
            }
          }
        },
        {
          "match": "\\b(requires|tags)\\s*:\\s*",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            }
          }
        },
        {
          "match": "\\b(order)\\s*:\\s*(sequential|parallel|random)",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.bench"
            }
          }
        },
        {
          "match": "\\b(mode)\\s*:\\s*\"?(auto|fixed)\"?",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.bench"
            }
          }
        },
        {
          "match": "\\b(fairness)\\s*:\\s*\"?(legacy|strict)\"?",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.bench"
            }
          }
        },
        {
          "match": "\\b(asyncSamplingPolicy)\\s*:\\s*\"?(timeBudgeted|fixedCap)\"?",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.bench"
            }
          }
        },
        {
          "match": "\\b(suiteType)\\s*:\\s*\"?(performance|memory)\"?",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.bench"
            }
          }
        },
        {
          "match": "\\b(runMode)\\s*:\\s*\"?(time|iterations|timeBased|iterationBased)\"?",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.bench"
            }
          }
        },
        {
          "match": "\\b(sameDataset)\\s*:\\s*(true|false)",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.boolean.bench"
            }
          }
        },
        {
          "match": "\\b(sink|memory|outlierDetection)\\s*:\\s*(true|false)",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            },
            "2": {
              "name": "constant.language.boolean.bench"
            }
          }
        },
        {
          "match": "\\b(baseline)\\s*:\\s*",
          "captures": {
            "1": {
              "name": "keyword.other.bench"
            }
          }
        },
        {
          "name": "string.quoted.double.bench",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.bench",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "meta.array.bench",
          "begin": "\\[",
          "end": "\\]",
          "patterns": [
            {
              "name": "string.quoted.double.bench",
              "begin": "\"",
              "end": "\"",
              "patterns": [
                {
                  "name": "constant.character.escape.bench",
                  "match": "\\\\."
                }
              ]
            },
            {
              "match": ",",
              "name": "punctuation.separator.bench"
            }
          ]
        }
      ]
    },
    "setup-go-block": {
      "name": "meta.block.setup.go.bench",
      "begin": "^(\\s*)(setup)\\s+(go)\\s*\\{\\s*$",
      "end": "^\\1\\}\\s*$",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.bench"
        },
        "3": {
          "name": "entity.name.language.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "#setup-import-go-block"
        },
        {
          "include": "#setup-declare-go-block"
        },
        {
          "include": "#setup-init-go-block"
        },
        {
          "include": "#setup-helpers-go-block"
        },
        {
          "include": "#comment"
        }
      ]
    },
    "setup-import-go-block": {
      "name": "meta.block.import.go.bench",
      "begin": "^(\\s*)(import)\\s*(\\()",
      "end": "^\\1\\)",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.setup.bench"
        },
        "3": {
          "name": "punctuation.definition.block.begin.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "source.go"
        }
      ]
    },
    "setup-declare-go-block": {
      "name": "meta.block.declare.go.bench",
      "begin": "^(\\s*)(declare)\\s*\\{",
      "end": "^\\1\\}",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.setup.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "source.go"
        }
      ]
    },
    "setup-init-go-block": {
      "name": "meta.block.init.go.bench",
      "begin": "^(\\s*)(init)\\s*\\{",
      "end": "^\\1\\}",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.setup.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "source.go"
        }
      ]
    },
    "setup-helpers-go-block": {
      "name": "meta.block.helpers.go.bench",
      "begin": "^(\\s*)(helpers)\\s*\\{",
      "end": "^\\1\\}",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.setup.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "source.go"
        }
      ]
    },
    "setup-ts-block": {
      "name": "meta.block.setup.ts.bench",
      "begin": "^(\\s*)(setup)\\s+(ts)\\s*\\{\\s*$",
      "end": "^\\1\\}\\s*$",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.bench"
        },
        "3": {
          "name": "entity.name.language.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "#setup-import-ts-block"
        },
        {
          "include": "#setup-declare-ts-block"
        },
        {
          "include": "#setup-init-ts-block"
        },
        {
          "include": "#setup-helpers-ts-block"
        },
        {
          "include": "#comment"
        }
      ]
    },
    "setup-import-ts-block": {
      "name": "meta.block.import.ts.bench",
      "begin": "^(\\s*)(import)\\s*\\{",
      "end": "^\\1\\}",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.setup.bench"
        }
      },
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.end.bench"
        }
      },
      "patterns": [
        {
          "include": "source.ts"
        }
      ]
    },
    "setup-declare-ts-block": {
      "name": "meta.block.declare.ts.bench",
      "begin": "^(\\s*)(declare)\\s*\\{",
      "end": "^\\1\\}",
      "beginCaptures": {
        "2": {
          "name": "keyword.control.setup.bench"
        }
      },
      "endCaptures": {
